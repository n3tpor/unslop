# Unslop Environment Configuration
# Copy this file to .env and configure the values for your environment

# Core SAM Configuration
NAMESPACE=unslop
SOLACE_DEV_MODE=false
USE_TEMPORARY_QUEUES=true

# Solace Broker Configuration
SOLACE_BROKER_URL=ws://localhost:8080
SOLACE_BROKER_USERNAME=default
SOLACE_BROKER_PASSWORD=default
SOLACE_BROKER_VPN=default

# WebUI Gateway Configuration  
FASTAPI_HOST=127.0.0.1
FASTAPI_PORT=8000
SESSION_SECRET_KEY=your-secret-key-here
WEBUI_GATEWAY_ID=unslop-webui

# Gateway Features
ENABLE_EMBED_RESOLUTION=true
GATEWAY_ARTIFACT_LIMIT_BYTES=10000000
SSE_MAX_QUEUE_SIZE=200

# Model Provider Configuration (example for custom LLM service)
# LLM_SERVICE_ENDPOINT=http://localhost:8000/v1
# LLM_SERVICE_API_KEY=your-api-key
# LLM_SERVICE_PLANNING_MODEL_NAME=gemini-2.5-pro
# LLM_SERVICE_GENERAL_MODEL_NAME=gemini-2.5-pro

# Image Generation Model (if needed)
# IMAGE_SERVICE_ENDPOINT=http://localhost:8000/v1
# IMAGE_SERVICE_API_KEY=your-image-api-key
# IMAGE_MODEL_NAME=dall-e-3

# Report Generation Model (if needed)
# LLM_REPORT_MODEL_NAME=gemini-2.5-pro

# Logging Configuration
LOG_LEVEL=INFO

# Additional Environment Variables
# Add any custom environment variables your agents might need
# CUSTOM_API_KEY=your-custom-api-key
# EXTERNAL_SERVICE_URL=http://localhost:9000
